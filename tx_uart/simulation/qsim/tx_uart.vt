module tx_uart_vlg_vec_tst();
// constants                                           
// general purpose registers
reg CLOCK_50;
reg KEY0;
reg KEY3;
reg [8:2] SW;
// wires                                               
wire count_enabe;
wire ctrl_counter;
wire [1:0] ctrl_state;
wire [6:0] HEX4;
wire [6:0] HEX5;
wire UART_TXD;

wire sampler;                             

// assign statements (if any)                          
tx_uart i1 (
// port map - connection between master ports and signals/registers   
	.CLOCK_50(CLOCK_50),
	.count_enabe(count_enabe),
	.ctrl_counter(ctrl_counter),
	.ctrl_state(ctrl_state),
	.HEX4(HEX4),
	.HEX5(HEX5),
	.KEY0(KEY0),
	.KEY3(KEY3),
	.SW(SW),
	.UART_TXD(UART_TXD)
);

// CLOCK_50
always
begin
	CLOCK_50 = 1'b0;
	CLOCK_50 = #10000 1'b1;
	#10000;
end 

// KEY0
initial
begin
	KEY0 = 1'b1;
	KEY0 = #2920000 1'b0;
	KEY0 = #20000 1'b1;
	KEY0 = #1220000 1'b0;
	KEY0 = #20000 1'b1;
end 

// KEY3
initial
begin
	KEY3 = 1'b1;
	KEY3 = #160000 1'b0;
	KEY3 = #20000 1'b1;
	KEY3 = #7120000 1'b0;
	KEY3 = #20000 1'b1;
end 

// SW[8]
initial
begin
	SW[8] = 1'b1;
end 

// SW[7]
initial
begin
	SW[7] = 1'b0;
end 

// SW[6]
initial
begin
	SW[6] = 1'b1;
end 

// SW[5]
initial
begin
	SW[5] = 1'b0;
end 

// SW[4]
initial
begin
	SW[4] = 1'b1;
end 

// SW[3]
initial
begin
	SW[3] = 1'b0;
end 

// SW[2]
initial
begin
	SW[2] = 1'b1;
end 

tx_uart_vlg_sample_tst tb_sample (
	.CLOCK_50(CLOCK_50),
	.KEY0(KEY0),
	.KEY3(KEY3),
	.SW(SW),
	.sampler_tx(sampler)
);

tx_uart_vlg_check_tst tb_out(
	.count_enabe(count_enabe),
	.ctrl_counter(ctrl_counter),
